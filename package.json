{
  "name": "vow-promise",
  "version": "0.0.0-semantically-released",
  "description": "A promise library",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "report-coverage": "codecov",
    "cover": "nyc npm t",
    "test": "nyc --reporter=lcov --reporter=text-summary mocha src/tests/*.js --require @babel/register",
    "semantic-release": "semantic-release",
    "build": "babel --copy-files --out-dir dist src --ignore tests",
    "prebuild": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnptmcdonald/vow.git"
  },
  "keywords": [
    "Promise"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "johnptmcdonald <johnptmcdonald@gmail.com> (http://www.johnptmcdonald.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnptmcdonald/vow/issues"
  },
  "homepage": "https://github.com/johnptmcdonald/vow#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-private-methods": "7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "chai": "4.2.0",
    "codecov": "3.8.3",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "jsdoc": "3.6.4",
    "json-loader": "0.5.7",
    "mocha": "7.1.1",
    "nyc": "15.0.1",
    "rimraf": "3.0.2",
    "semantic-release": "^17.0.7",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-private-methods"
    ]
  }
}
